   <xs:element name="text">
      <xs:annotation>
         <xs:documentation>Creates a text node or string.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:attribute name="disable-output-escaping" type="c:boolean">
