            <xs:group minOccurs="0" ref="c:sequence-constructor"/>
            <xs:choice>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" ref="c:catch"/>
                  <xs:element minOccurs="0" ref="c:finally"/>
               </xs:sequence>
