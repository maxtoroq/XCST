<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:type name='Foo'>
      <c:member name='bar'>
         <c:member name='@class'>
            <c:member name='m1'/>
         </c:member>
      </c:member>
   </c:type>

   <c:template name='c:initial-template'>
