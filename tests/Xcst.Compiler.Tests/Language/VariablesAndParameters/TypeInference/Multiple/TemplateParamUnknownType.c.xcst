<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:call-template name='foo'>
         <c:with-param name='a' value='new object[] { 2 }'/>
      </c:call-template>
   </c:template>

   <c:template name='foo'>
      <c:param name='a'>
         <c:object value='1'/>
         <c:object value='2'/>
      </c:param>
   </c:template>

</c:module>