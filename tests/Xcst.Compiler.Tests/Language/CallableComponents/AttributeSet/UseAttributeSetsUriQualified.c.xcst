<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#'
   xmlns:c='http://maxtoroq.github.io/XCST'
   xmlns:bar='urn:bar'>

   <c:attribute-set name='bar:bar'>
      <c:attribute name='bar'>bar</c:attribute>
