<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <foo>
         <c:for-each name='c' in='"hello".ToCharArray()'>
            <c:value-of value='c'/>
            <c:continue/>
            <c:text>!</c:text>
         </c:for-each>
      </foo>
   </c:template>

   <c:template name='expected'>
      <foo>hello</foo>
   </c:template>

</c:module>